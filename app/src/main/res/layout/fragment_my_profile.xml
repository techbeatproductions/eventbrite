<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".MyProfileFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/MyProfileGuideLine1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="24dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/MyProfileGuideLine2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="24dp" />

    <ImageView
        android:id="@+id/backBtnMyProfileFragIV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-24dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/back"
        app:layout_constraintStart_toStartOf="@id/MyProfileGuideLine1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profilePhotoMyProfileIV"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/circular_drawable_profile_holder"
        android:clipToOutline="true"
        android:scaleType="centerCrop"
        android:src="@drawable/event_image"
        app:layout_constraintEnd_toStartOf="@id/MyProfileGuideLine2"
        app:layout_constraintStart_toEndOf="@id/MyProfileGuideLine1"
        app:layout_constraintTop_toBottomOf="@id/backBtnMyProfileFragIV" />

    <TextView
        android:id="@+id/fullNameMyProfileTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/airbnb_cereal_w_xbd"
        android:text="David Kevin"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/MyProfileGuideLine2"
        app:layout_constraintStart_toStartOf="@+id/MyProfileGuideLine1"
        app:layout_constraintTop_toBottomOf="@id/profilePhotoMyProfileIV" />

    <TextView
        android:id="@+id/followingNumberMyProfileTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/airbnb_cereal_w_bk"
        android:text="350"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/followingLabelMyProfileFragTV"
        app:layout_constraintStart_toStartOf="@+id/followingLabelMyProfileFragTV"
        app:layout_constraintTop_toBottomOf="@id/fullNameMyProfileTV" />

    <TextView
        android:id="@+id/followingLabelMyProfileFragTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-8dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/airbnb_cereal_w_bk"
        android:text="Following"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@+id/fullNameMyProfileTV"
        app:layout_constraintTop_toBottomOf="@id/followingNumberMyProfileTV" />

    <View
        android:id="@+id/divider"
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/followersLabelMyProfileFragTv"
        app:layout_constraintEnd_toStartOf="@id/followersLabelMyProfileFragTv"
        app:layout_constraintStart_toEndOf="@id/followingLabelMyProfileFragTV"
        app:layout_constraintTop_toTopOf="@id/followersNumberMyProfileTV" />

    <TextView
        android:id="@+id/followersNumberMyProfileTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/airbnb_cereal_w_bk"
        android:text="346"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/followingNumberMyProfileTV"
        app:layout_constraintEnd_toEndOf="@+id/followersLabelMyProfileFragTv"
        app:layout_constraintStart_toStartOf="@+id/followersLabelMyProfileFragTv"
        app:layout_constraintTop_toTopOf="@id/followingNumberMyProfileTV"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/followersLabelMyProfileFragTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="-8dp"
        android:fontFamily="@font/airbnb_cereal_w_bk"
        android:text="Followers"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/followingLabelMyProfileFragTV"
        app:layout_constraintEnd_toEndOf="@+id/fullNameMyProfileTV"
        app:layout_constraintTop_toTopOf="@+id/followingLabelMyProfileFragTV"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>